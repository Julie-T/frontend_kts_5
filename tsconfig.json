{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "store/*": ["src/store/*"],
      "config/*": ["src/config/*"],
      "styles/*": ["src/styles/*"],
      "utils/*": ["src/utils/*"],
      "models/*": ["src/models/*"],
      "components/*": ["src/components/*"],

    },
    "noEmit": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "module": "es6",
    "target": "es6",
    "moduleResolution": "node",
    "jsx": "react",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
  },

  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
